      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 6 },
      "id": 11,
      "title": "Doğrulama İstekleri / dk",
      "type": "timeseries",
      "targets": [
        { "expr": "sum(rate(xslt_validations_total{application=\"mersel-xslt-service\"}[5m])) by (type, result)", "legendFormat": "{{type}} — {{result}}" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "unit": "ms", "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 } } },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 6 },
      "id": 12,
      "title": "Doğrulama Süresi (p95)",
      "type": "timeseries",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(xslt_validation_duration_seconds_bucket{application=\"mersel-xslt-service\"}[5m])) by (le, type)) * 1000", "legendFormat": "p95 {{type}}" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
