      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "textMode": "auto" },
      "title": "Toplam Doğrulama",
      "type": "stat",
      "targets": [
        { "expr": "sum(xslt_document_type_validations_total{application=\"mersel-xslt-service\"}) or vector(0)", "legendFormat": "Validations" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
